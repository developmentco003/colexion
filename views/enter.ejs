				<div class="header-left">
					<a href="index" class="logo">
						<img src="/img/logo.png" alt="Logo">
					</a>
					<a href="index" class="logo logo-small">
						<img src="/img/logo-small.png" alt="Logo" width="30" height="30">
					</a>
				</div>
				<!-- /Logo -->

				<a href="javascript:void(0);" id="toggle_btn">
					<i class="fe fe-text-align-left"></i>
				</a>


				<!-- Mobile Menu Toggle -->
				<a class="mobile_btn" id="mobile_btn">
					<i class="fa fa-bars"></i>
				</a>
				<!-- /Mobile Menu Toggle -->

				<!-- Header Right Menu -->
				<ul class="nav user-menu">


					<!-- User Menu -->
					<li class="nav-item dropdown has-arrow">
						<a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown">
							Admin
						</a>
						<div class="dropdown-menu">
							<a class="dropdown-item" href="/admin/logout">Logout</a>
						</div>
					</li>
					<!-- /User Menu -->

				</ul>
				<!-- /Header Right Menu -->

			</div>
			<!-- /Header -->

			<!-- Sidebar -->
			<div class="sidebar" id="sidebar">
				<div class="sidebar-inner slimscroll">
					<div id="sidebar-menu" class="sidebar-menu">
						<ul>
							<li class="menu-title">
								<span>Home</span>
							</li>
							<li>
								<a href="/latestupdates"><i class="fe fe-list-bullet"></i> <span>Latest Updates</span></a>
							</li>
							<li class=''>
								<a href="/features"><i class="fe fe-list-bullet"></i> <span>Sports and features</span></a>
							</li>
							<li>
								<a href="/about"><i class="fe fe-list-bullet"></i> <span>About us</span></a>
							</li>
							<li >
								<a href="/blogs"><i class="fe fe-list-bullet"></i> <span>Blogs</span></a>
							</li>
							<li class='active'>
								<a href="/enter"><i class="fe fe-list-bullet"></i> <span>Entertainment</span></a>
							</li>
							<li>
								<a href="/wait"><i class="fe fe-list-bullet"></i> <span>Waitlist</span></a>
							</li>
							<li >
								<a href="/staytune"><i class="fe fe-list-bullet"></i> <span>Staytune</span></a>
							</li>
							<li >
								<a href="/ads"><i class="fe fe-list-bullet"></i> <span>Advertisement</span></a>
							</li>
							<li >
								<a href="/video"><i class="fe fe-list-bullet"></i> <span>Video</span></a>
							</li>
							<li >
								<a href="/shopby"><i class="fe fe-list-bullet"></i> <span>Shop By</span></a>
							</li>


					</div>
				</div>
			</div>
			<!-- /Sidebar -->

			<!-- Page Wrapper -->
			<div class="page-wrapper">
				<div class="content container-fluid">

					<!-- Page Header -->
					<div class="page-header">
						<div class="row">
							<div class="col-12 col-md-12 col-lg-12 d-flex">
								<h3 class="page-title">Entertainment</h3>

							</div><div class="col-12 col-md-12 col-lg-12 d-flex">
								<a href="#Add_Doctor" data-toggle="modal"
									class="btn btn-primary float-right mt-2">Add</a>
							</div>
						</div>
					</div>


					<!-- /Page Header -->

					<div class="row">
						<div class="col-sm-12">
							<div class="card">
								<div class="card-body">
									<div class='row'>
										<% for(var i=0; i<data.length; i++) { %>
										<div class="col-lg-4 col-md-4">
											<div class="card flex-fill">
												<img alt="Card Image" src='<%- data[i].img %>' class="card-img-top">
												<div class="card-header">
													<h5 class="card-title mb-0"><%- data[i].name %></h5>
												</div>
												<div class="card-body">
													<p class="card-text">
														<strong>Type : </strong>  <%- data[i].type %>
														&emsp;&emsp;&emsp;
														<strong>Age : </strong>  <%- data[i].age %>
													</p>
													
													
													<div class="actions">
						
														<a data-toggle="modal" href="#edit<%- i+1 %>"
															class="btn btn-sm bg-success-light mr-2">
															<i class="fe fe-pencil"></i> Edit
														</a>
														<a class="btn btn-sm bg-danger-light"  href="/enter/delete/<%- data[i].id %>">
															<i class="fe fe-trash"></i> Delete
														</a>
													</div>
												</div>
											</div>
										</div>
										<% } %>

										

									</div>
								</div>
							</div>
						</div>
					</div>

				</div>
			</div>
			<!-- /Page Wrapper -->

	<!-- Add Modal -->

	<div class="modal fade" id="Add_Doctor" aria-hidden="true" role="dialog">
		<div class="modal-dialog modal-dialog-centered" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title">Add Person</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>
				<div class="modal-body">
					<form action="/enter" method="POST"  enctype='multipart/form-data'>
						<div class="row form-row">
							<div class="col-12 col-sm-6">
								<div class="form-group">
									<label>Name</label>
									<input name="name" type="text" class="form-control">
								</div>
							</div>
							<div class="col-12 col-sm-6">
								<div class="form-group">
									<label>Age</label>
									<input name="age" type="text" class="form-control">
								</div>
							</div>
							<div class="col-12 col-sm-6">
								<div class="form-group">
									<label>Type</label>
									<input name="type" type="text" class="form-control">
								</div>
							</div>
							<div class="col-12 col-sm-6">
								<div class="form-group">
									<label>Image</label>
									<input name="image" type="file" class="form-control">
									<input name="fallbackurl" hidden type="text" class="form-control"
													value="">
								</div>
							</div>
							
							

						</div>
						<button type="submit" class="btn btn-primary btn-block">Add Person</button>
					</form>
				</div>
			</div>
		</div>
	</div>
	<!-- /ADD Modal -->


			<!-- Edit Details Modal -->

			<!-- for loop start here -->
			<% for(var i=0; i<data.length; i++) { %>
			<div class="modal fade" id="edit<%- i+1 %>" aria-hidden="true" role="dialog">
				<div class="modal-dialog modal-dialog-centered" role="document">
					<div class="modal-content">
						<div class="modal-header">
							<h5 class="modal-title">Edit Person</h5>
							<button type="button" class="close" data-dismiss="modal" aria-label="Close">
								<span aria-hidden="true">&times;</span>
							</button>
						</div>
						<div class="modal-body">
							<form action="/enter/edit/<%- data[i].id %>" method="POST"  enctype='multipart/form-data'>
								<div class="row form-row">
									<div class="col-12 col-sm-6">
										<div class="form-group">
											<label>Name</label>
											<input name="name" type="text" class="form-control" value="<%- data[i].name %>" >
										</div>
									</div>
									<div class="col-12 col-sm-6">
										<div class="form-group">
											<label>Age</label>
											<input name="age" type="text" class="form-control"  value='<%- data[i].age %>'>
										</div>
									</div>
									<div class="col-12 col-sm-6">
										<div class="form-group">
											<label>Type</label>
											<input name="type" type="text" class="form-control" value="<%- data[i].type %>">
										</div>
									</div>
									<div class="col-12 col-sm-6">
										<div class="form-group">
											<label>Image</label>
											<input name="image" type="file" class="form-control">
											<input name="fallbackurl" hidden type="text" class="form-control"
															value="<%- data[i].img %>">
										</div>
									</div>
									
		
								</div>
								<button type="submit" class="btn btn-primary btn-block">Save Changes</button>
							</form>
						</div>
					</div>
				</div>
			</div>
			<% } %>

			<!-- copy -->

			<div class="modal fade" id="edit_doctor2" aria-hidden="true" role="dialog">
				<div class="modal-dialog modal-dialog-centered" role="document">
					<div class="modal-content">
						<div class="modal-header">
							<h5 class="modal-title">Edit Stats</h5>
							<button type="button" class="close" data-dismiss="modal" aria-label="Close">
								<span aria-hidden="true">&times;</span>
							</button>
						</div>
						<div class="modal-body">

							<form action="/admin/doctors/edit/" method="POST" enctype='multipart/form-data'>
								<div class="row form-row">

									<div class="col-12 col-sm-6">
										<div class="form-group">
											<label>Stats</label>
											<input name="name" type="text" class="form-control" value="">
										</div>
									</div>

								</div>
								<button type="submit" class="btn btn-primary btn-block">Save Changes</button>
							</form>
						</div>
					</div>
				</div>
			</div>

			<!-- for loop ends here -->

			<!-- /Edit Details Modal -->

			<!-- Delete Modal -->
			<div class="modal fade" id="delete_modal" aria-hidden="true" role="dialog">
				<div class="modal-dialog modal-dialog-centered" role="document">
					<div class="modal-content">
						<div class="modal-body">
							<div class="form-content p-2">
								<h4 class="modal-title">Delete</h4>
								<p class="mb-4">Are you sure want to delete?</p>
								<form action="/admin/doctor/delete/" method="POST">
									<button type="button" class="btn btn-primary">Save </button>
								</form>

								<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- /Delete Modal -->

		</div>
		<!-- /Main Wrapper -->

		<!-- jQuery -->
		<script src="/js/jquery-3.2.1.min.js"></script>

		<!-- Bootstrap Core JS -->
		<script src="/js/popper.min.js"></script>
		<script src="/js/bootstrap.min.js"></script>

		<!-- Slimscroll JS -->
		<script src="/plugins/slimscroll/jquery.slimscroll.min.js"></script>

		<!-- Datatables JS -->
		<script src="/plugins/datatables/jquery.dataTables.min.js"></script>
		<script src="/plugins/datatables/datatables.min.js"></script>

		<!-- Custom JS -->
		<script src="/js/script.js"></script>
		<script src="/js/edit.js"></script>

